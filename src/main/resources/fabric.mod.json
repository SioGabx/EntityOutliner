{
  "schemaVersion": 1,
  "id": "reentityoutliner",
  "version": "${version}",

  "name": "Re:Entity Outliner",
  "description": "Allows you to select entity types to outline, making them visible through all obstructions within render distance.",
  "authors": [
    "SioGabx, adamviola"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/reentityoutline",
    "sources": "https://github.com/SioGabx/ReEntityOutliner"
  },

  "license": "MIT",
  "icon": "assets/reentityoutliner/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.reentityoutliner.ReEntityOutliner"
    ],
    "modmenu": [
      "net.reentityoutliner.ui.ModMenuIntegration"
    ]
  },
  "mixins": [
    {
      "config": "reentityoutliner.mixins.json",
      "environment": "client"
    }
  ],

  "depends": {
    "fabricloader": ">=0.15.3",
    "fabric": "*",
    "fabric-lifecycle-events-v1": "*",
    "fabric-key-binding-api-v1": "*",
    "minecraft": ">=1.19"
  },
  "suggests": {
    "flamingo": "*"
  }
}
